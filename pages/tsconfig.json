{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "module": "es2022",
    "target": "es2022",
    "useDefineForClassFields": false,
    "rootDir": "..",
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@greycat/web",
    "types": ["vite/client"],
  },
  "include": ["**/*.ts", "**/*.tsx", "../src", "../jsx.d.ts"]
}
