{
  "name": "@greycat/ui",
  "version": "0.0.0",
  "module": "./dist/greycat.sdk.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/greycat.sdk.js"
    }
  },
  "license": "Apache-2.0",
  "files": [
    "dist/css",
    "dist/docs",
    "dist/types",
    "dist/greycat.sdk.js",
    "dist/greycat.sdk.min.js"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm build:types && pnpm build:esm && pnpm build:esm-min && pnpm build:css",
    "build:esm": "esbuild src/index.ts --bundle --format=esm --keep-names --outfile=dist/greycat.sdk.js --allow-overwrite",
    "build:esm-min": "esbuild src/index.ts --bundle --format=esm --keep-names --outfile=dist/greycat.sdk.min.js --minify --allow-overwrite",
    "build:types": "tsc",
    "build:css": "pnpm build:css:base && pnpm build:css:classless && pnpm build:css:full",
    "build:css:base": "lightningcss --minify --bundle --targets '>=0.25%' src/css/base.css -o dist/css/greycat.base.css",
    "build:css:classless": "lightningcss --minify --bundle --targets '>=0.25%' src/css/classless.css -o dist/css/greycat.classless.css",
    "build:css:full": "lightningcss --minify --bundle --targets '>=0.25%' src/css/full.css -o dist/css/greycat.css",
    "build:pages": "vite build",
    "build:docs": "vitepress build",
    "dev": "vite -c pages.config.ts",
    "dev:docs": "vitepress dev",
    "lint": "eslint src  --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "fmt": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@greycat/sdk": "https://get.greycat.io/files/sdk/js/dev/6.2/6.2.0-dev.tgz",
    "d3": "7.8.5",
    "maplibre-gl": "2.4.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "0.8.4",
    "@greycat/lang": "https://get.greycat.io/files/lang/dev/6.1/6.1.27-dev.tgz",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/d3": "7.4.0",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "esbuild": "0.18.17",
    "eslint": "8.46.0",
    "lightningcss-cli": "1.21.7",
    "prettier": "3.0.0",
    "rollup": "^3.29.2",
    "rollup-plugin-dts": "^6.0.2",
    "tslib": "^2.6.2",
    "typescript": "5.1.6",
    "vite": "4.4.8",
    "vite-plugin-dts": "3.5.2",
    "vitepress": "1.0.0-rc.13"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}