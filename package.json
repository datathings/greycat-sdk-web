{
  "name": "@greycat/web",
  "version": "0.0.0",
  "type": "module",
  "module": "./dist/greycat.esm.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/greycat.esm.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js"
    },
    "./css/*.css": "./dist/*.css",
    "./fonts/*.ttf": "./fonts/*.ttf",
    "./fonts/*.woff": "./fonts/*.woff"
  },
  "license": "Apache-2.0",
  "files": [
    "jsx.d.ts",
    "dist",
    "fonts",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm build:jsx && pnpm build:types && pnpm build:bundle && pnpm build:css",
    "build:jsx": "tsc -p tsconfig.jsx.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build:bundle": "node esbuild.config.js",
    "build:css": "lightningcss --minify --bundle --targets '>=0.25%' src/css/greycat.css -o dist/greycat.css",
    "build:playground": "tsc -p playground/tsconfig.json && vite build -c playground/vite.config.ts",
    "dev": "vite -c playground/vite.config.ts",
    "lint": "eslint src  --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "fmt": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@greycat/sdk": "https://get.greycat.io/files/sdk/js/dev/7.0/7.0.71-dev.tgz",
    "@shoelace-style/shoelace": "2.15.1",
    "d3": "7.8.5",
    "dockview-core": "1.10.1"
  },
  "devDependencies": {
    "@tabler/icons": "2.47.0",
    "@types/d3": "7.4.0",
    "esbuild": "0.20.2",
    "eslint": "8.46.0",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "prettier": "3.3.3",
    "lightningcss": "1.25.1",
    "lightningcss-cli": "1.25.1",
    "tslib": "2.6.3",
    "typescript": "5.4.3",
    "vite": "5.3.5",
    "http-proxy": "1.18.1"
  }
}