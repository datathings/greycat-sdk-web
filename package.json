{
  "name": "@greycat/web",
  "version": "0.0.0",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/greycat.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js"
    },
    "./css/greycat.css": "./dist/css/greycat.css",
    "./css/greycat.base.css": "./dist/css/greycat.base.css",
    "./css/greycat.classless.css": "./dist/css/greycat.classless.css",
    "./fonts/Raleway-Medium.ttf": "./public/Raleway-Medium.ttf"
  },
  "license": "Apache-2.0",
  "files": [
    "jsx.d.ts",
    "dist/css",
    "dist/docs",
    "dist/esm",
    "dist/greycat.js",
    "dist/greycat.min.js",
    "dist/jsx-dev-runtime.js",
    "dist/jsx-runtime.js",
    "public/Raleway-Medium.ttf"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm build:jsx && pnpm build:esm && pnpm build:bundle && pnpm build:bundle-min && pnpm build:css",
    "build:esm": "tsc",
    "build:bundle": "esbuild src/index.ts --bundle --format=esm --keep-names --outfile=dist/greycat.js --allow-overwrite",
    "build:bundle-min": "esbuild src/index.ts --bundle --format=esm --keep-names --outfile=dist/greycat.min.js --minify --allow-overwrite",
    "build:jsx": "tsc -p tsconfig.jsx.json",
    "build:css": "pnpm build:css:slim && pnpm build:css:full",
    "build:css:slim": "lightningcss --minify --bundle --targets '>=0.25%' src/css/greycat.slim.css -o dist/css/greycat.slim.css",
    "build:css:full": "lightningcss --minify --bundle --targets '>=0.25%' src/css/greycat.css -o dist/css/greycat.css",
    "build:pages": "vite build -c pages.config.ts",
    "build:docs": "vitepress build",
    "dev": "vite -c pages.config.ts",
    "dev:docs": "vitepress dev",
    "lint": "eslint src  --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "fmt": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@greycat/sdk": "https://get.greycat.io/files/sdk/js/dev/6.3/6.3.2-dev.tgz",
    "@picocss/pico": "1.5.10",
    "d3": "7.8.5",
    "maplibre-gl": "2.4.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "0.8.4",
    "@greycat/lang": "https://get.greycat.io/files/lang/dev/6.1/6.1.27-dev.tgz",
    "@types/d3": "7.4.0",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "esbuild": "0.18.17",
    "eslint": "8.46.0",
    "lightningcss-cli": "1.22.0",
    "prettier": "3.0.0",
    "typescript": "5.2.2",
    "vite": "4.4.8",
    "vitepress": "1.0.0-rc.13"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}