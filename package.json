{
  "name": "@greycat/ui",
  "version": "0.0.0",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist/bundle",
    "dist/esm"
  ],
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist docs",
    "build": "pnpm build:esm && pnpm build:bundle && pnpm build:css && cp -R fonts dist/",
    "build:esm": "tsc",
    "build:bundle": "esbuild --bundle src/bundle.ts --platform=browser --outfile=dist/bundle/greycat.ui.js --minify",
    "build:css": "pnpm build:css:base && pnpm build:css:classless && pnpm build:css:full",
    "build:css:base": "lightningcss --minify --bundle --targets '>=0.25%' src/css/base.css -o dist/bundle/greycat.ui.base.css",
    "build:css:classless": "lightningcss --minify --bundle --targets '>=0.25%' src/css/classless.css -o dist/bundle/greycat.ui.classless.css",
    "build:css:full": "lightningcss --minify --bundle --targets '>=0.25%' src/css/full.css -o dist/bundle/greycat.ui.css",
    "build:pages": "vite build",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "watch": "esbuild --bundle src/index.ts --platform=browser --outfile=dist/bundle/greycat.ui.js --sourcemap --watch",
    "lint": "eslint src  --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "fmt": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "d3": "7.8.5",
    "maplibre-gl": "2.4.0"
  },
  "devDependencies": {
    "@types/d3": "7.4.0",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "esbuild": "0.18.17",
    "eslint": "8.46.0",
    "lightningcss-cli": "1.21.7",
    "prettier": "3.0.0",
    "typescript": "5.1.6",
    "vite": "4.4.8",
    "vite-plugin-dts": "3.5.2",
    "vitepress": "1.0.0-beta.7",
    "vue": "3.3.4"
  },
  "peerDependencies": {
    "@greycat/sdk": "https://get.greycat.io/files/sdk/js/dev/6.1/6.1.79-dev.tgz"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}