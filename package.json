{
  "name": "@greycat/ui",
  "version": "0.0.0",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist/bundle",
    "dist/esm"
  ],
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist docs",
    "build": "pnpm build:esm && pnpm build:bundle && cp -R fonts dist/",
    "build:esm": "tsc --emitDeclarationOnly && esbuild --bundle src/index.ts --platform=browser --outdir=dist/esm",
    "build:bundle": "esbuild --bundle src/bundle.ts --platform=browser --outfile=dist/bundle/greycat.ui.js --minify",
    "build:app": "vite build",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "watch": "esbuild --bundle src/index.ts --platform=browser --outfile=dist/bundle/greycat.ui.js --sourcemap --watch",
    "lint": "eslint src  --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "fmt": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@greycat/sdk": "https://get.greycat.io/files/sdk/js/dev/6.1/6.1.30-dev.tgz",
    "d3": "7.8.5",
    "maplibre-gl": "2.4.0"
  },
  "devDependencies": {
    "@types/d3": "7.4.0",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "esbuild": "0.18.17",
    "eslint": "8.46.0",
    "prettier": "3.0.0",
    "typescript": "5.1.6",
    "vite": "4.4.8",
    "vite-plugin-dts": "3.5.2",
    "vitepress": "1.0.0-beta.7",
    "vue": "3.3.4"
  },
  "publishConfig": {
    "@greycat:registry": "https://hub.datathings.com/api/v4/projects/289/packages/npm/"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}