{
  "name": "@greycat/web",
  "version": "0.0.0",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/greycat.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js"
    },
    "./css/*.css": "./dist/css/*.css",
    "./fonts/*.ttf": "./public/*.ttf",
    "./fonts/*.woff": "./public/*.woff"
  },
  "license": "Apache-2.0",
  "files": [
    "dist/css",
    "dist/docs",
    "dist/esm",
    "dist/greycat.js",
    "dist/greycat.min.js",
    "dist/greycat.iife.min.js",
    "jsx.d.ts",
    "dist/jsx-dev-runtime.js",
    "dist/jsx-dev-runtime.d.ts",
    "dist/jsx-runtime.js",
    "dist/jsx-runtime.d.ts",
    "public/OpenSans-Regular.ttf",
    "public/OpenSans-Italic.ttf",
    "public/OpenSans-Bold.ttf",
    "public/OpenSans-Regular.woff",
    "public/OpenSans-Italic.woff",
    "public/OpenSans-Bold.woff"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm build:jsx && pnpm build:esm && pnpm build:bundle && pnpm build:bundle:esm && pnpm build:bundle:iife && pnpm build:css",
    "build:esm": "tsc",
    "build:bundle": "esbuild src/index.ts --bundle --format=esm --keep-names --outfile=dist/greycat.js --allow-overwrite",
    "build:bundle:esm": "esbuild src/index.ts --bundle --format=esm --keep-names --outfile=dist/greycat.min.js --minify --allow-overwrite",
    "build:bundle:iife": "esbuild src/index.ts --bundle --format=iife --keep-names --outfile=dist/greycat.iife.min.js --minify --allow-overwrite",
    "build:jsx": "tsc -p tsconfig.jsx.json",
    "build:css": "pnpm build:css:base && pnpm build:css:full",
    "build:css:base": "lightningcss --minify --bundle --targets '>=0.25%' src/css/greycat.base.css -o dist/css/greycat.base.css",
    "build:css:full": "lightningcss --minify --bundle --targets '>=0.25%' src/css/greycat.css -o dist/css/greycat.css",
    "build:pages": "vite build -c pages.config.ts",
    "build:docs": "vitepress build",
    "dev": "vite -c pages.config.ts",
    "dev:docs": "vitepress dev",
    "lint": "eslint src  --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "fmt": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@greycat/sdk": "https://get.greycat.io/files/sdk/js/stable/6.10/6.10.14-stable.tgz",
    "d3": "7.8.5",
    "dockview-core": "1.10.1"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "0.8.4",
    "@greycat/lang": "https://get.greycat.io/files/lang/stable/6.10/6.10.45-stable.tgz",
    "@picocss/pico": "1.5.10",
    "@types/d3": "7.4.0",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@tabler/icons": "2.47.0",
    "esbuild": "0.18.17",
    "eslint": "8.46.0",
    "http-proxy": "1.18.1",
    "lightningcss": "1.22.0",
    "lightningcss-cli": "1.22.0",
    "prettier": "3.0.0",
    "typescript": "5.2.2",
    "vite": "4.4.8",
    "vitepress": "1.0.0-rc.22"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "packageManager": "pnpm@8.8.0"
}
