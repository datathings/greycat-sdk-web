{
  "name": "@greycat/ui",
  "version": "0.0.0",
  "type": "module",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist docs",
    "build": "pnpm build:esm && pnpm build:bundle && pnpm build:manifest && cp -R fonts dist/",
    "build:esm": "tsc",
    "build:bundle": "esbuild --bundle src/bundle.ts --platform=browser --outfile=dist/bundle/greycat.ui.js --minify --sourcemap",
    "build:manifest": "cem analyze --globs 'src/components/**/*.ts' --exclude '**/index.ts' --exclude '**/bundle.ts' && mv custom-elements.json dist/",
    "build:docs": "vite build",
    "watch": "esbuild --bundle src/index.ts --platform=browser --outfile=dist/bundle/greycat.ui.js --sourcemap --watch",
    "lint": "eslint src  --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "fmt": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@greycat/lib-std": "5.9.83-testing",
    "@greycat/rpc": "5.9.83-testing",
    "@greycat/utils": "5.9.83-testing",
    "d3": "7.6.1",
    "maplibre-gl": "2.4.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "0.6.6",
    "@greycat/cli": "5.9.83-testing",
    "@types/d3": "7.4.0",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "esbuild": "0.15.10",
    "eslint": "8.25.0",
    "prettier": "^3.0.0",
    "typescript": "5.0.4",
    "vite": "^4.3.9"
  },
  "publishConfig": {
    "@greycat:registry": "https://hub.datathings.com/api/v4/projects/289/packages/npm/"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}